<tool id="modlamp_analysis" name="modlamp analysis" version="1.1.2">
  <description>for any numerical column</description>
  <requirements>
    <requirement type="package" version="1.0.3">pandas</requirement>
    <requirement type="package" version="1.76">biopython</requirement> 
    <requirement type="package" version="3.5">nltk</requirement> 
    <requirement type="package" version="3.7.1">gensim</requirement>
    <requirement type="package" version="0.23.1">scikit-learn</requirement>
    <requirement type="package" version="1.18.4">numpy</requirement>
  </requirements>
  <stdio>
    <exit_code range="1" level="fatal" />
  </stdio>
    <command detect_errors="exit_code"><![CDATA[

        python '$__tool_directory__/Word2Vec.py' -M '$model' -I '$input1' -R '$row' -C '$column' -O '$outFile'
        
    ]]></command>

  <inputs>
    <param name="model" type="data" format="fasta" argument= "--InFile"/>
    <param name="input1" type="data" format="fasta" argument= "--InFile"/>
    <param name="row" label="row" help="row"/>
    <param name="column" label="row" value="200" help="column"/>
  </inputs>

  <outputs>
    <data name='output1' format='tabular' label="out.tsv" />
  </outputs>

  <tests>
    <test>
      <param name="input" value="1.bed"/>
      <param name="cond" value="c2"/>
      <output name="out_file1" file="gsummary_out1.tabular"/>
    </test>
  </tests>
  <help>

  </help>
</tool>



